---
apiVersion: app/v1
kind: Deployment
metadata:
  name: tomcat-deployment
  labels:
    app: tomcat
spec:
  replicas: 3
  selector:
    matchlabels:
      app: tomcat
template:
  metadata:
    labels:
      app: tomcat
  spec:
    containers:
      - name: tomcat
        image: diya0311/mynewimage:111
        port:
          - containerport: 8080

